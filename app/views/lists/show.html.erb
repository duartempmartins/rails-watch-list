<h1>Hello from list <%= @list.id %></h1>
<% @list.bookmarks.each do |bookmark| %>
<h2> <%= bookmark.movie.title %> </h2>
<p> <%= bookmark.comment %> </p>
<p> <%= link_to "Delete this bookmark", bookmark_path(bookmark), data: {turbo_method: :delete, turbo_confirm: "R U Sure?"}  %></p>
<% end %>
<p> <%= link_to "Add bookmark",  new_list_bookmark_path(@list) %></p>
<p> <%= link_to "Back to lists page",  lists_path %></p>
